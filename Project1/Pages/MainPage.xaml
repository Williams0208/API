<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Project1.Pages.MainPage">
    <ContentPage.Content>
        <StackLayout Margin="10" VerticalOptions="StartAndExpand">
            <StackLayout>
                <Entry Text="{Binding UserName}" Placeholder="Nombre de usuario"/>
                <Entry IsPassword="True" Text="{Binding Password}" Placeholder="Contraseña"/>
                <Button Command="{Binding RegisterCommand}" Text="Login"/>
                
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>